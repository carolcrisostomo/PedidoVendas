unit uProduto;

interface

type
  TProduto=class
  private
    FCodigo:Integer;
    FDescricao:string;
    FValor:Currency;
  public
    property Codigo:Integer read FCodigo write FCodigo;
    property Descricao:string read FDescricao write FDescricao;
    property Valor:Currency read FValor write FValor;
  end;

implementation

end.